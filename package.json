{
  "name": "youtube2anki",
  "displayName": "Youtube2anki",
  "version": "0.0.1",
  "description": "An extension to synchronize youtube subtitle to Anki",
  "author": "lei",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.1",
    "@plasmohq/storage": "^1.9.0",
    "immer": "^10.0.3",
    "jotai": "^2.6.0",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/xml2js": "^0.4.14",
    "daisyui": "^4.4.20",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "manifest": {
    "permissions": [
      "storage"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/y.js",
          "assets/sub-url.js"
        ],
        "matches": [
          "https://www.youtube.com/*"
        ]
      }
    ]
  }
}
